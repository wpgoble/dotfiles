# If you come from bash you might have to change your $PATH.
# export PATH=$HOME/bin:/usr/local/bin:$PATH

# Path to your oh-my-zsh installation.
export ZSH="/Users/goble/.oh-my-zsh"
PATH=$PATH:/usr/local/bin; export PATH
# Set name of the theme to load --- if set to "random", it will
# load a random theme each time oh-my-zsh is loaded, in which case,
# to know which specific one was loaded, run: echo $RANDOM_THEME
# See https://github.com/ohmyzsh/ohmyzsh/wiki/Themes
ZSH_THEME="norm"

plugins=(git)

source $ZSH/oh-my-zsh.sh

alias -g home="cd "
alias -g pip="pip3"
alias -g python="python3"
alias -g school="cd ~/Documents/school/"
alias -g work="cd ~/Documents/ardupilot/"
alias -g udemy="cd ~/Desktop/Udemy/"
export LESS="-R"
alias -g hammer="vim ~/.hammerspoon/init.lua"
alias -g zshrc="vim ~/.zshrc"
alias -g vimrc="vim ~/.vimrc"
alias -g netflix='open https://netflix.com'
alias -g hbo="open https://play.hbonow.com/page/urn:hbo:page:home"
alias -g hulu="open https://hulu.com"